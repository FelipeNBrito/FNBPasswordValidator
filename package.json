{
  "name": "fnbpasswordvalidator",
  "version": "1.0.12",
  "description": "Powerfull password validation for js",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "babel-node src/index.js",
    "pretest": "npm run build",
    "test": "mocha --reporter spec",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "build": "babel src -d dist",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FelipeNBrito/FNBPasswordValidator.git"
  },
  "keywords": [
    "password",
    "validation",
    "password",
    "validator",
    "secure"
  ],
  "author": "Felipe N. Brito <me@felipenbrito.com> (http://felipenbrito.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FelipeNBrito/FNBPasswordValidator/issues"
  },
  "homepage": "https://github.com/FelipeNBrito/FNBPasswordValidator#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.11",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3"
  },
  "dependencies": {
    "i18next": "^19.8.0",
    "i18next-sync-fs-backend": "^1.1.1"
  }
